package ringoram;

public class MetaData {

	protected int bucket_access_counter;		// number of times this bucket has been accessed
	protected int[] dummy_pos;					// dummy positions in the bucket
	boolean[] dummy;					// to check if a block in a bucket is a dummy block
	protected int [][] bucket_pos_map;
	


	MetaData(int num_dummy_blocks, int bucket_size){
		dummy_pos = new int[num_dummy_blocks];
		dummy = new boolean[bucket_size];
		bucket_pos_map = new int[bucket_size][1];
		
	}
}
