package message;

import message.Message.MessageType;
import server.QueryLog;

public class GetQLog extends Message{
	
	QueryLog qlog;
	
	GetQLog(int ClientID,int MessageID,int num_clients) {super(ClientID,MessageID);qlog = new QueryLog(num_clients);}
	public MessageType getMessageType(){return MessageType.Ping;}
	public int getMessageID (){ return super.MessageID;}
	public int getClientID() {return super.clientID;}
	public QueryLog getQLog() {return qlog;}

}
