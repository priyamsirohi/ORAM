package Operations;

import ringoam.*;


public class UploadOperation extends Operations 
{
	protected node dataItem = null;
	
	public UploadOperation(long r, String k, node di) { super(r, k); dataItem = di; }

	@Override
	public OperationType getType() { return OperationType.UPLOAD; }
	
	public node getDataItem() { return dataItem; }
}
