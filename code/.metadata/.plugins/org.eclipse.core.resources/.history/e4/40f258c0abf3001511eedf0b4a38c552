package message;


public class GetPath extends Message{

	private int leaf_id;
	
	GetPath(int MessageID, int ClientID, int leaf_id) { super(MessageID,ClientID); this.leaf_id = leaf_id;} 
	
	
	@Override
	public MessageType getMessageType(){return MessageType.GetNodes;}
	public int getMessageID (){ return super.MessageID;}
	public int getClientID() {return super.clientID;}
	public int getLeaf_ID() {return this.leaf_id;}
		

}
